# 请求部分
float64[] joint_positions  # 关节位置数组，单位：度
float64[] cartesian_pose   # 笛卡尔位姿数组 [x, y, z, rx, ry, rz]
int32 move_type            # 移动类型：0=关节移动, 1=笛卡尔移动
float64 velocity           # 速度百分比 0-100
float64 acceleration       # 加速度百分比 0-100
---
# 响应部分
bool success               # 操作是否成功
string message             # 返回消息或错误描述